<div class="h-full ml-13 mt-14 mb-10 md:ml-64">

<div class="relative w-full h-full max-w-2xl md:h-auto mx-auto mt-16">
       
            <form action="/admin/updatedAddress/" method="post" class="relative bg-white rounded-lg shadow dark:bg-gray-700" >
              
            <div class="flex items-start justify-between p-4 border-b rounded-t dark:border-gray-600">
                    <h3 class="text-xl font-semibold text-gray-900 dark:text-white"> Update adress
                    </h3>
                     <h4 class="mb-3 text-lg font-medium text-green-500 dark:text-white">#{success}</h4>
                    
            </div>
               
                <div class="p-6 space-y-6">
                    <div class="grid grid-cols-6 gap-6">
                        <div class="col-span-6 sm:col-span-3">
                            <label for="house_number" class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">House number:</label>
                            <input type="text" name="housenumber" value="#{updatingAddress.housenumber}" id="housenumber" class="shadow-sm bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-600 focus:border-blue-600 block w-full p-2.5 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" placeholder="92/102" ="">
                           
                               <input type = "hidden" id="userId" name="userId" value="#{updatingAddress._id}" />
                        </div>
                        <div class="col-span-6 sm:col-span-3">
                            <label for="colony" class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">Colony:</label>
                            <input type="text" name="colony" id="colony" value="#{updatingAddress.colony}" class="shadow-sm bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-600 focus:border-blue-600 block w-full p-2.5 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" placeholder="shivam nagar" ="">
                        </div>
                        <div class="col-span-6 sm:col-span-3">
                            <label for="Landmark" class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">Landmark:</label>
                            <input type="text" name="Landmark" id="Landmark"  value="#{updatingAddress.landmark}" class="shadow-sm bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-600 focus:border-blue-600 block w-full p-2.5 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" placeholder="styilevilla" ="">
                        </div>
                        <div class="col-span-6 sm:col-span-3">
                            <label for="city" class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">City:</label>
                            <input type="text" name="city" id="city" value="#{updatingAddress.city}" class="shadow-sm bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-600 focus:border-blue-600 block w-full p-2.5 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" placeholder="didwana" ="">
                        </div>
                        <div class="col-span-6 sm:col-span-3">
                            <label for="District" class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">District:</label>
                            <input type="text" name="district" id="district" value="#{updatingAddress.district}" class="shadow-sm bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-600 focus:border-blue-600 block w-full p-2.5 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" placeholder="Nagour" ="">
                        </div>
                        <div class="col-span-6 sm:col-span-3">
                            <label for="State" class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">State:</label>
                            <input type="text" name="State" id="State" value="#{updatingAddress.state}" class="shadow-sm bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-600 focus:border-blue-600 block w-full p-2.5 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" placeholder="Rajasthan" ="">
                        </div>
                        <div class="col-span-6 sm:col-span-3">
                            <label for="Zipcode" class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">Zip code:</label>
                            <input type="Zip code" name="zipcode" id="Zipcode" value="#{updatingAddress.zipcode}" class="shadow-sm bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-600 focus:border-blue-600 block w-full p-2.5 dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" placeholder="302020" ="">
                        </div>
                       
                        <div class=" items-center p-6 space-x-2 border-t border-gray-200 rounded-b dark:border-gray-600">
                    <button type="submit" class="text-white bg-red-700 hover:bg-red-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800">Add</button>
                </div>
                    </div>
</div>




